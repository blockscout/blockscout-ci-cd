  "scripts": {
    "build": "webpack",
    "lint": "eslint --ext .ts .",
    "test": "npm run build && export $(cat $CONFIG | xargs) && ./k6 --verbose run --tag testid=${K6_TEST_ID} --log-output=${K6_LOG_OUTPUT_STRING} dist/all.test.suite.js",
